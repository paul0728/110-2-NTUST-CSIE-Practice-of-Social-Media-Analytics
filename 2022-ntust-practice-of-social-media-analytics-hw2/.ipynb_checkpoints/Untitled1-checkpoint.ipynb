   "source": [
    "def xrange(n):\n",
    "    x = 0\n",
    "    while x != n:\n",
    "        yield x\n",
    "        x += 1\n",
    "\n",
    "for n in xrange(10):\n",
    "    print(n) \n",
    "g = xrange(2)\n",
    "type(g)\n",
    "\n"
   ]
